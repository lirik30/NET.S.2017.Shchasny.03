    <expectedResult>15</expectedResult>
  </TestCase>
  <TestCase>
    <inNumber>15</inNumber>
    <numberToInsert>15</numberToInsert>
    <from>1</from>
    <to>4</to>
    <expectedResult>31</expectedResult>
  </TestCase>
  <TestCase>
    <inNumber>15</inNumber>
    <numberToInsert>-15</numberToInsert>
    <from>1</from>
    <to>4</to>
    <expectedResult>15</expectedResult>
  </TestCase>
  <TestCase>
    <inNumber>-8</inNumber>
    <numberToInsert>-15</numberToInsert>
    <from>1</from>
    <to>4</to>
    <expectedResult>-6</expectedResult>
  </TestCase>
  <TestCase>
    <inNumber>int.MaxValue</inNumber>
    <numberToInsert>int.MaxValue</numberToInsert>
    <from>3</from>
    <to>5</to>
    <expectedResult>int.MaxValue</expectedResult>
  </TestCase>
