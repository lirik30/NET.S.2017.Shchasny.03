    <to>0</to>
    <expectedResult>9</expectedResult>
  </TestCase>
  <TestCase>
    <inNumber>0</inNumber>
    <numberToInsert>15</numberToInsert>
    <from>30</from>
    <to>30</to>
    <expectedResult>1073741824</expectedResult>
  </TestCase>
  <TestCase>
    <inNumber>0</inNumber>
    <numberToInsert>15</numberToInsert>
    <from>0</from>
    <to>30</to>
    <expectedResult>15</expectedResult>
  </TestCase>
  <TestCase>
    <inNumber>15</inNumber>
    <numberToInsert>-15</numberToInsert>
    <from>0</from>
    <to>4</to>
